{
    "name": "fndmiranda/data-migration",
    "description": "Synchronize and migrate data between the application and the database",
    "keywords": [
      "laravel",
      "migration",
      "migrate",
      "seed",
      "seeder",
      "data",
      "database",
      "eloquent"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Fernando Miranda",
            "email": "fndmiranda@gmail.com"
        }
    ],
    "require": {
      "php": ">=7.0.0",
      "laravel/framework": "^5.5"
    },
    "require-dev": {
      "phpunit/phpunit": "^5.7|6.2|^7.0",
      "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
    },
    "autoload": {
      "psr-4": {
        "Fndmiranda\\DataMigration\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Fndmiranda\\DataMigration\\Tests\\": "tests"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Fndmiranda\\DataMigration\\DataMigrationServiceProvider"
        ]
      }
    },
    "scripts": {
      "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
